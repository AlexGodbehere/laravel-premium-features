{
    "name": "alexgodbehere/laravel-premium-features",
    "description": "Premium feature manager for Laravel 8+",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Godbehere",
            "email": "alex.godbehere@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Migrations"
        ],
        "psr-4": {
            "AlexGodbehere\\LaravelPremiumFeatures\\": "src/",
            "AlexGodbehere\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexGodbehere\\LaravelPremiumFeatures\\AchievementsServiceProvider"
            ]
        }
    },
    "require": {
        "ramsey/uuid": "^3 || ^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "laravel/laravel": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.4"
    }
}
